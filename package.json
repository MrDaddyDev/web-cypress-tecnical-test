{
  "name": "web-cypress-tecnical-test",
  "version": "1.0.0",
  "description": "Este proyecto está hecho para una prueba técnica para la empresa Omni.pro llevándolo acabo en Cypress con Cucumber para su escritura de casos de prueba",
  "main": "index.js",
  "scripts": {
    "start cypress": "cypress open",
    "allure:generate": "cypress run && allure generate --clean ./cypress/test_supports/allure-results -o ./allure-report",
    "allure:open": "allure open ./allure-report"
  },
  "author": "carlos.piragua.contacto@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^21.0.3",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "allure-cypress": "^3.0.7",
    "cypress": "^13.16.1",
    "esbuild": "^0.24.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "eslint": "^9.17.0"
  }
}
